<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\calibr\calibr.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Resources\HtmlLibrary.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>HtmlLibrary.resx</DependentUpon>
    </Compile>
    <Compile Update="Resources\UriLibrary.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>UriLibrary.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Resources\HtmlLibrary.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>HtmlLibrary.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\UriLibrary.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>UriLibrary.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

</Project>
